{
  "version": 2,
  "functions": {
    "api/socket.js": {
      "maxDuration": 30
    },
    "api/rooms.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 5
    },
    "api/admin.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
